directory_path <- "\\Users\extatorresg\Documents\prueba santi"
# readxl para manear excel
if (!requireNamespace("readxl", quietly = TRUE)) {
install.packages("readxl")
}
library(readxl)
# funcion para verificar el numero de columnas en la hoja 2
check_worksheet_columns <- function(file_path) {
wb <- read_excel(file_path, sheet = 2)
return(ncol(wb))
}
# carpeta de los archivos
directory_path <- "C:\\Users\\extatorresg\\Documents\\prueba santi"
#cConsidera todos los archivos en la carpeta
file_paths <- list.files(directory_path, pattern = "\\.xlsx$", full.names = TRUE)
# Loop
for (file_path in file_paths) {
# nombre del archivo
file_name <- basename(file_path)
# numero de columnas en hoja 2
column_count <- check_worksheet_columns(file_path)
# Verificar visualmente
cat("Archivo:", file_name, "| Numero de columnas en hoja 2:", column_count, "\n")
}
